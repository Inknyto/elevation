
.icons-container {
  position: fixed;
  top: 0;
  display: flex;
  justify-content: space-between;
  width: 100%;
  padding: 10px;
}

.icon {
  display: flex;
  align-items: center;
  gap: 10px;
  cursor: pointer;
  z-index: 999; 
  padding: 10px;
}
.icon svg{
  padding: 5px;
  border-radius:50px;
  transition: all 0.3s ease 0s;
}
.icon:hover svg{
  box-shadow: 0px 15px 20px #0000005c;
  transform: translateY(-7px);
}
.icons-right{
display: flex;
flex-direction: column;
align-items:flex-end;

}


